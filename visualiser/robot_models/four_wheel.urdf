<!--Model used from http://www.romin.upm.es/wiki/index.php?title=ROS%3ATutorials/Build_Your_Own_URDF_Robot-->

<robot name="robot">
  <!-- Base link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.195" rpy="0 0 0" />
      <geometry>
          <box size="1 0.5 0.25"/>
      </geometry>
      <material name="yellow">
        <color rgba="0.8 0.8 0 1"/>
      </material>
    </visual>
  </link>
  <!-- Front Right Wheel -->
  <link name="f_r_wheel">
    <visual>
      <origin xyz="0 0 0.195" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black">
        <color rgba="0.05 0.05 0.05 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_f_r_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="f_r_wheel"/>
    <origin xyz="0.25 -0.30 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>  
  <!-- Back Right Wheel -->
  <link name="b_r_wheel">
    <visual>
      <origin xyz="0 0 0.195" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_b_r_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="b_r_wheel"/>
    <origin xyz="-0.25 -0.30 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>  
  <!-- Front Left Wheel -->
  <link name="f_l_wheel">
    <visual>
      <origin xyz="0 0 0.195" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_f_l_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="f_l_wheel"/>
    <origin xyz="0.25 0.30 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
  <!-- Back Left Wheel -->
  <link name="b_l_wheel">
    <visual>
      <origin xyz="0 0 0.195" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_b_l_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="b_l_wheel"/>
    <origin xyz="-0.25 0.30 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>

  <!-- Laser front -->
  <link name="laser_front">
    <visual>
      <origin xyz="0 0 0.195" rpy="0.0 0 0" />
      <geometry>
          <box size="0.1 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="0.7 0 0.2 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_laser_front" type="continuous">
    <parent link="base_link"/>
    <child link="laser_front"/>
    <origin xyz="0.45 0.0 0.15" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>

  <!-- Laser front right -->
  <link name="laser_front_right">
    <visual>
      <origin xyz="0 0 0.195" rpy="0.0 0 0" />
      <geometry>
          <box size="0.1 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="0.7 0 0.2 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_laser_front_right" type="continuous">
    <parent link="base_link"/>
    <child link="laser_front_right"/>
    <origin xyz="0.45 -0.2 0.15" rpy="0 0 -0.785398" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>

  <!-- Laser front left -->
  <link name="laser_front_left">
    <visual>
      <origin xyz="0 0 0.195" rpy="0.0 0 0" />
      <geometry>
          <box size="0.1 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="0.7 0 0.2 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_laser_front_left" type="continuous">
    <parent link="base_link"/>
    <child link="laser_front_left"/>
    <origin xyz="0.45 0.2 0.15" rpy="0 0 0.785398" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>

  <!-- Laser right -->
  <link name="laser_right">
    <visual>
      <origin xyz="0 0 0.195" rpy="0.0 0 0" />
      <geometry>
          <box size="0.1 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="0.7 0 0.2 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_laser_right" type="continuous">
    <parent link="base_link"/>
    <child link="laser_right"/>
    <origin xyz="0 -0.2 0.15" rpy="0 0 -1.57070796" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>

  <!-- Laser left -->
  <link name="laser_left">
    <visual>
      <origin xyz="0 0 0.195" rpy="0.0 0 0" />
      <geometry>
          <box size="0.1 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="0.7 0 0.2 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_laser_left" type="continuous">
    <parent link="base_link"/>
    <child link="laser_left"/>
    <origin xyz="0 0.2 0.15" rpy="0 0 1.57070796" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
</robot>
